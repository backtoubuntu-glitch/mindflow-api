{
  \"version\": 2,
  \"name\": \"mindflow-ai-platform\",
  \"builds\": [
    {
      \"src\": \"package.json\",
      \"use\": \"@vercel/static-build\",
      \"config\": { \"distDir\": \"dist\" }
    }
  ],
  \"routes\": [
    {
      \"src\": \"/(.*)\",
      \"dest\": \"/index.html\"
    }
  ],
  \"env\": {
    \"REACT_APP_NAME\": \"MindFlow AI Learning Platform\",
    \"REACT_APP_DEVELOPER\": \"MindTech\",
    \"REACT_APP_VERSION\": \"1.0.0\",
    \"REACT_APP_MISSION\": \"Making quality AI-powered education accessible to every African child\"
  }
}
